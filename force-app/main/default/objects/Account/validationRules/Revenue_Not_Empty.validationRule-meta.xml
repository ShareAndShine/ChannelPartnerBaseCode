<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Revenue_Not_Empty</fullName>
    <active>true</active>
    <errorConditionFormula>ISBLANK( AnnualRevenue )</errorConditionFormula>
    <errorDisplayField>AnnualRevenue</errorDisplayField>
    <errorMessage>Annual Revenue for an account should not be empty</errorMessage>
</ValidationRule>
